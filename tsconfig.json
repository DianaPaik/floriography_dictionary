{
  // ##"extends": "@tsconfig/react-native/tsconfig.json"
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "preserve",
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "*": ["src/*"],
      "tests": ["tests/*"],
      "@components/*": ["components/*"],
    "lib": ["esnext", "dom"],
    }
  }
}